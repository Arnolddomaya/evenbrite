<% provide(:title, "Sign up")%>


<div class="row">

  <div class="col-md-6 col-md-offset-3">
    <h1>Sign up</h1>
    <%= form_for @user do |f| %>

      <%= f.label :Id %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "Sign up!", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
